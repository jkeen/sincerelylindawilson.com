---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Sincerely, Linda Wilson</title>
  <link href="http://sincerelylindawilson.com/atom.xml" rel="self"/>
  <link href="http://sincerelylindawilson.com/"/>
  <updated>{{site.time | date_to_xmlschema }}</updated>
  <id>http://sincerelylindawilson.com/</id>
  <author>
    <name>Linda Wilson</name>
    <email></email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="http://sincerelylindawilson.com/letters/{{post.category}}"/>
    <updated>{{post.date | date_to_xmlschema }}</updated>
    <id>http://sincerelylindawilson.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>